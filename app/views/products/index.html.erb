<h1>Jeff's Favorite Things</h1>

<div class="row">
<div class="span12">


<%= render 'cart' %>


<% @products.each do |product| %>
<ul class="productindex">
  <li class="imageindex"><%= image_tag(product.imageurl) %></li>
  <li><%= product.name %></li>
  <li><%= number_to_currency(product.price) %></li>
  <td><%= link_to 'Show', product %></td>
  <li><%= link_to 'Edit', edit_product_path(product) %></li>
</ul>
<% end %>
<br/>


<%= link_to 'New Product', new_product_path %>

</div>






</div>